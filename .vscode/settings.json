{
  // File Configs
  "files.associations": {
    "*.css": "tailwindcss",
    "*.scss": "tailwindcss",
    "*.sass": "tailwindcss"
  },

  // Tailwind CSS
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.experimental.classRegex": [
    ["([\"'`][^\"'`]*.*?[\"'`])", "[\"'`]([^\"'`]*).*?[\"'`]"]
  ]
}
